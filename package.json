{
  "name": "educard",
  "version": "1.0.0",
  "description": "A simple educational web forum built with Node.js, Express, and PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no tests specified\" && exit 0",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "db:analyze": "node src/utils/db-analyze.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [
    "forum",
    "education",
    "web",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "connect-flash": "^0.1.1",
    "dompurify": "^3.3.0",
    "dotenv": "^16.6.1",
    "ejs": "^3.1.0",
    "express": "^4.18.0",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.17.0",
    "express-validator": "^7.0.0",
    "helmet": "^8.1.0",
    "highlight.js": "^11.11.1",
    "isomorphic-dompurify": "^2.30.0",
    "jsdom": "^26.1.0",
    "marked": "^15.0.12",
    "node-cache": "^5.1.2",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.0"
  },
  "devDependencies": {
    "axios": "^1.13.2",
    "eslint": "^8.48.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "sequelize-cli": "^6.6.3"
  }
}
